<script type="text/javascript">

    var baseUrl = "https://api.themoviedb.org/3/";

    $(function (){
        //accordeon
        $('.accordeon-title').on('click', function () {
            $(this).toggleClass('active');
            $(this).next().slideToggle();
        });
        getGenre();

    });

    function getMovies()
    {
        var surl = baseUrl+"search/movie?api_key=aa646e218fda2ff0dfcd4d3a3cb55e37&language=en-US/&query=" + $("#Location").val() + "&page=1&include_adult=false";
        $.ajax({
            type: 'GET',
            url: surl,
            crossDomain: true,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
               getResult(data);
            },
            error: function (xhr, status, error) { alert(error); },
            async: false,
            cache: false
        });

    }

    function getGenre() {
       // var url = "/Movie/GetGenre";
        //$.get(url, function (data) {
        //    console.log(data);
        //    for (var i = 0; i < data[0].Genres.length; i++) {
        //        $("#genreDiv ul").append('<li id="' + data[0].Genres[i].Id +'" onclick="getMoviesByGenre(event)">' + data[0].Genres[i].Name + '</li>');
        //    }
        //});
        var surl = baseUrl +"genre/movie/list?api_key=aa646e218fda2ff0dfcd4d3a3cb55e37&language=en-US";
        $.ajax({
            type: 'GET',
            url: surl,
            crossDomain: true,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                console.log(data);
                for (var i = 0; i < data.genres.length; i++) {
                    $("#genreDiv ul").append('<li id="' + data.genres[i].id + '" onclick="getMoviesByGenre(event)">' + data.genres[i].name + '</li>');
                }
            },
            error: function (xhr, status, error) { alert(error); },
            async: false,
            cache: false
        });
    }

    function getMoviesByGenre(e) {
        var url = "/Movie/GetMovieByGenre";
        var mve = e.currentTarget.id;
        $.get(url, function (data) {
            getResult(data);
        })
        
    }

    function getResult(data)
    {
        for (var i = 0; i < data.results.length; i++) {
            $("#Result .row").append(`<div class="col-sm-4 portfolio-entry" >
                  <div class="image">
                  <img alt="" src="https://image.tmdb.org/t/p/w500` + data.results[i].poster_path + `"> </div>
                     <h3 class="title" >` + data.results[i].title + `</h3>
                      <div class="subtitle">`+ data.results[i].release_date + `</div>
                      <div class="subtitle"> Rate :`+ data.results[i].vote_average + `</div>
                     <div class="description">` + data.results[i].overview + `</div>
                  </div>
                          `);
        }
    }


</script>

